++
++	DisableSequence [
++		cset "name='Stereo ADC MIXL ADC1 Switch' off"
++		cset "name='Stereo ADC MIXR ADC1 Switch' off"
++		cset "name='RECMIXL BST1 Switch' off"
++		cset "name='RECMIXR BST1 Switch' off"
